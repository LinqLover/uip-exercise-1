{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build pscom-cli",
			"type": "shell",
			"command": "/usr/bin/make",
			"args": ["CONFIG+=debug"],
			"options": {
				"cwd": "${workspaceFolder}/pscom-cli"
			},
			"problemMatcher": [
				"$gcc"
